"*":
  core:
    customFileTypes:
      "source.ini": [
        ".buckconfig"
        ".flowconfig"
      ]
      "source.json": [
        ".arcconfig"
        "BUCK.autodeps"
      ]
      "source.python": [
        "BUCK"
      ]
    destroyEmptyPanes: false
    disabledPackages: [
      "autocomplete-atom-api"
      "background-tips"
      "dev-live-reload"
      "exception-reporting"
      "language-clojure"
      "language-go"
      "language-java"
      "language-less"
      "language-mustache"
      "language-objective-c"
      "language-perl"
      "language-php"
      "language-python"
      "language-sql"
      "language-toml"
      "metrics"
      "open-on-github"
      "release-notes"
      "styleguide"
      "timecop"
      "welcome"
      "vim-mode"
      "deprecation-cop"
      "package-generator"
      "linter-rubocop"
      "activate-power-mode"
      "ide-flow"
      "nuclide"
      "linter-haml"
      "linter-jshint"
    ]
    excludeVcsIgnoredPaths: false
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "*.meta"
    ]
    projectHome: "~/devel"
  editor:
    fontFamily: "Monaco"
    invisibles: {}
    showInvisibles: true
  "file-watcher":
    promptWhenFileHasChangedOnDisk: false
  "language-babel":
    suppressTranspileOnSaveMessages: true
  linter: {}
  "linter-jshint": {}
  "linter-rubocop": {}
  "markdown-preview":
    grammars: [
      "source.gfm"
      "source.litcoffee"
      "text.plain"
      "text.plain.null-grammar"
    ]
  minimap:
    absoluteMode: true
  "one-dark-ui": {}
  "spell-check":
    grammars: [
      "source.gfm"
      "text.git-commit"
      "text.plain"
      "text.plain.null-grammar"
      "source.ruby"
      "source.js"
    ]
  "the-closer":
    closeWindowTogetherWithLastTab: false
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
