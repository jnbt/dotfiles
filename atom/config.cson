"*":
  welcome:
    showOnStartup: false
  core:
    disabledPackages: [
      "autocomplete-atom-api"
      "background-tips"
      "dev-live-reload"
      "exception-reporting"
      "language-clojure"
      "language-go"
      "language-java"
      "language-less"
      "language-mustache"
      "language-objective-c"
      "language-perl"
      "language-php"
      "language-python"
      "language-sql"
      "language-toml"
      "metrics"
      "open-on-github"
      "release-notes"
      "styleguide"
      "timecop"
      "welcome"
      "vim-mode"
      "deprecation-cop"
      "package-generator"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "Thumbs.db"
      "*.meta"
    ]
    projectHome: "~/devel"
    destroyEmptyPanes: false
  "tree-view":
    hideIgnoredNames: true
  editor:
    showInvisibles: true
    invisibles: {}
    fontFamily: "Monaco"
    fontSize: 18
  "one-dark-ui":
    layoutMode: "Compact"
  "linter-rubocop": {}
  linter: {}
  "language-babel":
    suppressTranspileOnSaveMessages: true
  minimap:
    absoluteMode: true
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
  "markdown-preview":
    grammars: [
      "source.gfm"
      "source.litcoffee"
      "text.plain"
      "text.plain.null-grammar"
    ]
  "spell-check":
    grammars: [
      "source.gfm"
      "text.git-commit"
      "text.plain"
      "text.plain.null-grammar"
    ]
  "the-closer":
    closeWindowTogetherWithLastTab: false
  "linter-jshint": {}
